(define $read-file
  (lambda [$file]
    (do { [$port (open-input-file file)]
          (read-content port)}
      (close-input-port port))))

(define $read-content
  (lambda [$file]
    (do { [$x (eof-port? file)]
          (if x (print "End") (get-line file))})))

;; (match x something
;;   {[#f (get-line file)]})

(define $get-line
  (lambda [$file]
    (do { [$c (read-char-from-port file)]
          (write-char c)}
      (read-content file))))

(define $main
  (lambda [$args]
    (do {[(read-file (nth 1 args))]}
      )))